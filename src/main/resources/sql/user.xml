<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserRepository">
	<select id="getUserInfo" resultType="User">
		SELECT 
			'안지표' as name, 
			31 as age 
		FROM dual
	</select>
</mapper>